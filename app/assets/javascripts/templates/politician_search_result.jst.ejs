<img src=<%= photo_url %> class="large-4 medium-4 small-columns 4">
<span id="hide-bio"><%=bioguide_id%></span>
<div class="large-8 medium-8 small-8 columns">
  <p class="result-text">
    <span class="search-state"> <%= state %> </span>
    <span class="search-name"><%= title %> <%= firstname %> <%= lastname %> (<%= party %>)</span>
    <%if (title === "Rep"){ %>
      <span class="search-type">House</span>
    <%} else if (title ==="Sen") {%>
      <span class="search-type">Senate</span>
    <%}%>
  </p>
</div>
